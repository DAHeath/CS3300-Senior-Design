<p id="notice"><%= notice %></p>

<ul class="nav nav-pills">
  <li role="presentation" class="active"><a href="#">Home</a></li>
  <li role="presentation"><a href="#">Browse Teams</a></li>
  <li role="presentation"><a href="#">Messages</a></li>
  <li role="presentation">
	<%= link_to 'Preferences', edit_student_path(@student) %> 
  </li>
</ul>

<div class="panel panel-primary">
	<div class="panel-heading">
		<div class="panel-heading">
			<h3 class="panel-title">
				Team Information for
				<%= @student.first_name %>
				<%= @student.last_name %>
			</h3>
		</div>
	</div>
	
	<div class="panel-body">
		<div class="list-group">
		  <a href="#" class="list-group-item">
			<h4 class="list-group-item-heading">Team Name</h4>
			<p class="list-group-item-text">
				<strong>Team Name Goes Here</strong>
			</p>
		  </a>
		</div>
		
		<div class="list-group">
		  <a href="#" class="list-group-item">
			<h4 class="list-group-item-heading">Team Members</h4>
			<p class="list-group-item-text">
				<ul>
					<li><strong>Name 1</strong></li>
					<li><strong>Name 2</strong></li>
					<li><strong>Name 3</strong></li>
					<li><strong>Name 4</strong></li>
					<li><strong>Name 5</strong></li>
				</ul>
			</p>
		  </a>
		</div>
		
		<div class="list-group">
		  <a href="#" class="list-group-item">
			<h4 class="list-group-item-heading">Point of Contact</h4>
			<p class="list-group-item-text">
				<strong>Name:</strong><br />
				<strong>Email:</strong>
			</p>
		  </a>
		</div>
	</div>
</div>

<div class="panel panel-primary">
	<div class="panel-heading">
		<div class="panel-heading">
			<h3 class="panel-title">Project Information</h3>
		</div>
	</div>
	
	<div class="panel-body">
		<div class="list-group">
		  <a href="#" class="list-group-item">
			<h4 class="list-group-item-heading">Active Project</h4>
			<p class="list-group-item-text">
				<strong>Project Name:</strong><br />
				<strong>Project Details:</strong>
			</p><br />
			<div class="alert alert-info" role="alert">
				Your team currently has no active project.
			</div>
		  </a>
		</div>
		
		<div class="list-group">
		  <a href="#" class="list-group-item">
			<h4 class="list-group-item-heading">Active Bids</h4><br />
			<p class="list-group-item-text">
				<table class="table" style="border: 1px solid rgb(221, 221, 221)">
					<thead>
						<tr>
							<th>Bid #</th>
							<th>Priority</th>
							<th>Project Name</th>
							<th>Sponsor</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>1</td>
							<td>2</td>
							<td>Sample Name 1</td>
							<td>Sample Sponsor 1</td>
						</tr>
						<tr>
							<td>2</td>
							<td>1</td>
							<td>Sample Name 2</td>
							<td>Sample Sponsor 2</td>
						</tr>
					</tbody>
				</table>
			</p><br />
			<div class="alert alert-info" role="alert">
				Your team currently has no active bids.
			</div>
		  </a>
		</div>
	</div>
</div>

<ul class="nav nav-pills">
	<li role="presentation">
		<%= link_to 'Back', students_path %>
	</li>
</ul>
