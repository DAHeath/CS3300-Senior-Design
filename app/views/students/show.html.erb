<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @student.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @student.last_name %>
</p>

<p>
  <strong>Team:</strong>
  <%= @student.team_id %>
</p>

<p><strong>Potential Teams:</strong></p>
<% @student.join_team_contracts.each do |t| %>
  <p>
    <%= t.team.name %>
  </p>
<% end %>

<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', students_path %>
